#ifndef SKIPLIST_H_
#define SKIPLIST_H_

#include "workercommon.h"

struct steaminode {

	void *element;

	void *pre;
	void *post;
	void *upper;
	void *down;
};

struct leaveinode {

	void *element;

	void *pre;
	void *post;
};

struct subskiplist {

	struct steaminode *head;
};

struct skiplist {

	int count;
	int layer;
};

#endif /* SKIPLIST_H_ */
