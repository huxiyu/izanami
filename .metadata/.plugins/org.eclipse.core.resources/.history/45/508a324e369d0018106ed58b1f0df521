#include "networkserver.h"

#include <stdlib.h>

struct networkserver *initnetworkserver(networkserverconfig config) {

	struct networkserver *server = (struct networkserver *) malloc(
			sizeof(struct networkserver));

	if (server == NULL) {
		exit(EXIT_FAILURE);
	}

	config(server);

	return server;
}
