#include "common.h"
#include "config.h"
#include "dictionary.h"
#include "iniparser.h"
#include "izanami-master.h"

#include "stdlib.h"

void configmaster(struct networkserver *server) {

	dictionary *dict = getdict();
	char *host = iniparser_getstring(dict, IZANAMI_MASTER_ETH, "0.0.0.0");
	int port = iniparser_getint(dict, IZANAMI_MASTER_PORT, 7000);
	server->executor = NULL;
}
